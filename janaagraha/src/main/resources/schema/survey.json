{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "Survey",
  "required": [
    "id",
    "surveyName",
    "category"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "Id of the survey",
      "default": "",
      "pattern": "^([A-Za-z0-9]{1,64})$"
    },
    "surveyName": {
      "$id": "#/properties/surveyName",
      "type": "string",
      "title": "Name of the Survey - Optional",
      "default": "",
      "pattern": "^([A-Za-z0-9]{1,64})$"
    },
    "category": {
      "$ref": "./category.json"
    }
  }
}